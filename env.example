# API Configuration
REACT_APP_API_BASE_URL=https://api.vaulto.ai
REACT_APP_HYPERLIQUID_API=https://api.hyperliquid.xyz
REACT_APP_CHAINLINK_API=https://api.chainlink.org

# WebSocket Configuration
REACT_APP_WS_URL=wss://ws.vaulto.ai

# Zapier Webhook Configuration (for Slack notifications)
# Get your webhook URL from Zapier after creating a new webhook trigger
# Example: https://hooks.zapier.com/hooks/catch/123456/abcdef/
ZAPIER_WEBHOOK_URL=https://hooks.zapier.com/hooks/catch/123456/abcdef/

# Analytics (optional)
REACT_APP_GA_TRACKING_ID=GA_TRACKING_ID

# Feature Flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_ANALYTICS=false
